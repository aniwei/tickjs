#!/usr/bin/env node
const commander = require('commander');

const { TickVersionManager } = require('../dist/tvm');

commander
  .command('init')
  .action(
    TickVersionManager
      .sharedInstance()
      .command('init')
  )

commander
  .command('start')
  .action(
    TickVersionManager
      .sharedInstance()
      .command('start', true)
  )

commander.parse(process.argv);