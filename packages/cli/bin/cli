#!/usr/bin/env node
const commander = require('commander');

const { 
  TickCLI,
  Commands
} = require('../dist/TickCLI');

const cli = new TickCLI();

commander
  .command(Commands.INIT)
  .action(cli.init)

commander
  .command(Commands.START)
  .action(cli.start)

commander 
  .command(Commands.LS)
  .action(cli.ls);

commander.parse(process.argv);