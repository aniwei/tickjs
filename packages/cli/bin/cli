#!/usr/bin/env node
const commander = require('commander');

const { TickVersionManager } = require('../dist/tvm');

commander
  .command('init')
  .action(
    TickVersionManager
      .sharedInstance()
      .init()
  )

commander
  .command('start')
  .action(
    TickVersionManager
      .sharedInstance()
      .start()
  )

commander.parse(process.argv);