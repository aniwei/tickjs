.inner-1 {
  height: 50px;
  background: green;
  padding: 10px;
  display: flex;
}

.inner-2 {
  height: 20px;
  background: blue;
}

.-tickjs-circulate-parent {
  box-sizing: border-box;
}
.-tickjs-circulate-parent::after {
  clear: both;
}

.-tickjs-circulate {
  float: left;
  all: inherit;
  padding: 0;
  margin: 0;
  width: 100%;
  height: 100%;
}

.inner-1-2 {
  flex: 1;
  margin: 0 10px;
  background: yellow;
}